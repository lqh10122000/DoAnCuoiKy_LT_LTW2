    <div class="col-12 row">
      <% for(let i = 0; i < Movies.length; i++) { %>
      <div class="col-6 rap__item">
        <div class="row">
          <div class="col-2 Home__logo">
            <img src="/movie/picturePoster/<%- Movies[i].id  %>" alt="" />
          </div>
          <div class="col-10">
            <h3><%- Movies[i].name %></h3>
            <p><%- Movies[i].time %></p>

            <% for(let j = 0; j < ShowTimes.length; j++) { %> <%
            if(ShowTimes[j].movieId == Movies[i].id) { %> <%
            if(ShowTimes[j].hour < 10) { %> <% if(ShowTimes[j].minute < 10) { %>

            <a
              href="/ticket/?idM=<%- Movies[i].id %>&idT=<%-ShowTimes[j].theaterId %>&idTC=<%-ShowTimes[j].theaterClusterId %>&idTime=<%- ShowTimes[j].showTimeId %>"
              >0<%- ShowTimes[j].hour %>:0<%- ShowTimes[j].minute %></a
            >

            <% } else { %>

            <a
              href="/ticket/?idM=<%- Movies[i].id %>&idT=<%-ShowTimes[j].theaterId %>&idTC=<%-ShowTimes[j].theaterClusterId %>&idTime=<%- ShowTimes[j].showTimeId %>"
              >0<%- ShowTimes[j].hour %>:<%- ShowTimes[j].minute %></a
            >

            <% } %> <% } else { %> <% if(ShowTimes[j].minute < 10) { %>

            <a
              href="/ticket/?idM=<%- Movies[i].id %>&idT=<%-ShowTimes[j].theaterId %>&idTC=<%-ShowTimes[j].theaterClusterId %>&idTime=<%- ShowTimes[j].showTimeId %>"
              ><%- ShowTimes[j].hour %>:0<%- ShowTimes[j].minute %></a
            >

            <% } else { %>

            <a
              href="/ticket/?idM=<%- Movies[i].id %>&idT=<%-ShowTimes[j].theaterId %>&idTC=<%-ShowTimes[j].theaterClusterId %>&idTime=<%- ShowTimes[j].showTimeId %>"
              ><%- ShowTimes[j].hour %>:<%- ShowTimes[j].minute %></a
            >

            <% } %> <% } %> <% } %> <% } %>
          </div>
        </div>
      </div>

      <% } %>
