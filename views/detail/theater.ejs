<div
  class="Ctr__InTro"
  style="background-image: url(../../assets/img/bhd-star-background.png)"
>
  <div class="bgBlack"></div>
  <div class="Ctr__content row">
    <div class="col-12 col-md-6">
      <div class="ctr__img">
        <img src="../../assets/img/bhd-star-bitexco.png" />
      </div>
      <div class="ctr__text">
        <h2><%- TheaterClusters.name %></h2>
        <p><%- TheaterClusters.address %></p>
        <button>Mua Vé</button>
      </div>
    </div>
    <div class="col-12 col-md-6 theater__map">
      <%- TheaterClusters.addressMaps %>
    </div>
  </div>
</div>
