<div class="signup">
    <div class="signup__content">
        <img src="../../assets/img/bg2.jpg" alt="" />
        <div class="signup__form">
            <h3>Register Now</h3>
            <form action="#" method="post">
                <div class="signup__input">
                    <input type="text" id="displayName" name="displayName" required="" />
                    <label>Username</label>
                </div>
                <div class="signup__input">
                    <input type="text" id="email" name="email" required="" />
                    <label>E-mail</label>

                </div>
                <div class="signup__input">
                    <input type="text" id="phone" name="phone" required="" />
                    <label>Number Phone</label>

                </div>
                <div class="signup__input">
                    <input type="password" id="password" name="password" required="" />
                    <label>Password</label>

                </div>
                <div class="signup__input">
                    <input type="password" id="password" name="password" required="" />
                    <label>Confirm Password</label>

                </div>
                <div class="signup__btn text-right">
                    <button button type="submit" class="btn btn-primary">Register</button>
                </div>
            </form>
            <div class="mt-3">
                <p style="color: black">
                    Already  have an account?
                    <a href="/auth/login"><span style="color: rgb(35, 236, 220); font-weight: 600;text-decoration: none;"
                      >Sign in</span
                    ></a
                  >
                </p>
              </div>
        </div>
    </div>
</div>