<div class="Home">
    <div class="Home__carousel">
        <div id="carouselHome" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselHome" data-slide-to="0" class="active"></li>
                <li data-target="#carouselHome" data-slide-to="1"></li>
                <li data-target="#carouselHome" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="../assets/img/lat-mat-48h.png" class="d-block w-100" alt="..." />
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselHome" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselHome" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
        <div class="Home__search">
            <div class="row">
                <div class="col-3 Home__item">
                    <div class="form-group">
                        <select class="form-control form-control-sm" placeholder="Phim">
                            <option hidden selected>Phim</option>
                            <% for(var i=0; i < listMovie.length; i++) { %>
                                <option>
                                    <%- listMovie[i].name %>
                                </option>
                                <% } %>
                        </select>
                    </div>
                </div>
                <div class="col-2 Home__item">
                    <div class="form-group">
                        <select class="form-control form-control-sm" placeholder="Phim">
                            <option hidden selected>Rạp</option>
                            <% for(var i=0; i < listTheaterCluster.length; i++) { %>
                                <option>
                                    <%- listTheaterCluster[i].name %>
                                </option>
                                <% } %>
                        </select>
                    </div>
                </div>
                <div class="col-2 Home__item">
                    <div class="form-group">
                        <select class="form-control form-control-sm" placeholder="Phim">
                            <option hidden selected>Ngày Xem</option>
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                        </select>
                    </div>
                </div>
                <div class="col-2 Home__item">
                    <div class="form-group">
                        <select class="form-control form-control-sm" placeholder="Phim">
                            <option hidden selected>Suất Chiếu</option>
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                        </select>
                    </div>
                </div>
                <div class="col-3 Home__item">
                    <button class="btn btn-danger">Mua Vé Ngay</button>
                </div>
            </div>
        </div>
    </div>
    <div class="Home__List">
        <div class="Home__title">
            <ul class="nav nav-tabs row" id="myTab" role="tablist">
                <li class="nav-item col-4" role="presentation" style="text-align: right">
                    <a class="nav-link active" id="dangChieu-tab" data-toggle="tab" href="#dangChieu" role="tab"
                        aria-controls="dangChieu" aria-selected="true">Đang Chiếu</a>
                </li>
                <li class="nav-item col-4" role="presentation" style="text-align: center">
                    <a class="nav-link" id="sapChieu-tab" data-toggle="tab" href="#sapChieu" role="tab"
                        aria-controls="sapChieu" aria-selected="false">Sắp Chiếu</a>
                </li>
                <li class="nav-item col-4" role="presentation">
                    <a class="nav-link" id="yeuThich-tab" data-toggle="tab" href="#yeuThich" role="tab"
                        aria-controls="yeuThich" aria-selected="false">Phim Yêu Thích</a>
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="dangChieu" role="tabpanel" aria-labelledby="dangChieu-tab">
                    <div class="row">
                        <% for(var i=0; i < listMovie.length; i++) { %>
                            <% if(listMovie[i].premiereDate - datenow <= 0 ) { %>
                                <a href="/detail/?id=<%- listMovie[i].id %>" class="Home__item col-3">
                                    <div class="Home__img">
                                        <img src="/movie/picturePoster/<%- listMovie[i].id  %>" alt="" />
                                        <div class="blackHome"></div>
                                    </div>
                                    <div class="Home__text">
                                        <h2>
                                            <span>
                                                <%- listMovie[i].name.slice(0,1); %>
                                            </span>
                                            <%- listMovie[i].name %>
                                        </h2>
                                        <p>
                                            <%- listMovie[i].time %> phút
                                        </p>
                                        <div class="buttonHome">
                                            <button class="btn btn-danger">Mua Vé</button>
                                        </div>
                                    </div>
                                </a>
                            <% } %>                           
                        <% } %>
                    </div>
                </div>
                <div class="tab-pane fade" id="sapChieu" role="tabpanel" aria-labelledby="sapChieu-tab">
                    <div class="row">
                        <% for(var i=0; i < listMovie.length; i++) { %>
                            <% if(listMovie[i].premiereDate - datenow > 0 ) { %>
                                <a href="/detail/?id=<%- listMovie[i].id %>" class="Home__item col-3">
                                    <div class="Home__img">
                                        <img src="/movie/picturePoster/<%- listMovie[i].id  %>" alt="" />
                                        <div class="blackHome"></div>
                                    </div>
                                    <div class="Home__text">
                                        <h2>
                                            <span>
                                                <%- listMovie[i].name.slice(0,1); %>
                                            </span>
                                            <%- listMovie[i].name %>
                                        </h2>
                                        <p>
                                            <%- listMovie[i].time %> phút
                                        </p>
                                        <div class="buttonHome">
                                            <button class="btn btn-danger">Mua Vé</button>
                                        </div>
                                    </div>
                                </a>
                            <% } %>                           
                        <% } %>
                    </div>
                </div>
                <div class="tab-pane fade" id="yeuThich" role="tabpanel" aria-labelledby="yeuThich-tab">
                    <div class="row">
                        <% for(var i=0; i < listMovieLike.length; i++) { %>
                            <% if(listMovieLike[i].likes > 0 ) { %>
                                <a href="/detail/?id=<%- listMovieLike[i].id %>" class="Home__item col-3">
                                    <div class="Home__img">
                                        <img src="/movie/picturePoster/<%- listMovieLike[i].id  %>" alt="" />
                                        <div class="blackHome"></div>
                                    </div>
                                    <div class="Home__text">
                                        <h2>
                                            <span>
                                                <%- listMovieLike[i].name.slice(0,1); %>
                                            </span>
                                            <%- listMovieLike[i].name %>
                                        </h2>
                                        <p>
                                            <%- listMovieLike[i].time %> phút
                                        </p>
                                        <div class="buttonHome">
                                            <button class="btn btn-danger">Mua Vé</button>
                                        </div>
                                    </div>
                                </a>
                            <% } %>                           
                        <% } %>
                    </div>                                 
                </div>
            </div>
        </div>
    </div>
    <div class="Home__CumRap">



        <% for(let i = 0; i < theaters.length; i++) { %>

            <div class="Home__rap row">
                <div class="Home__logo col-2">
                    <img src="../assets/logo/<%- theaters[i].name %>.png" />
                </div>
                <div class="col-10">
                    <div class="row">


                        
                        <% for(let j = 0; j < theaterClusters.length; j++) { %>

                            <% if(theaters[i].id == theaterClusters[j].theaterClusterId ) { %>
                                <div class="col-3 rap__item">

                                    <h3><span class="BHD"><%- theaterClusters[j].name %></span></h3>
                                    
                                    <a href="/detail/theater/?idT=<%- theaters[i].id %>&idTC=<%- theaterClusters[j].id %>">[Chi tiết]</a>
                                </div>
                            <% } %>

                        
                            
                        <% } %>
                    </div>
                </div> 
            </div> 

        <% } %>


